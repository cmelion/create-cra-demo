{
  "name": "react-cra-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "autodux": "^5.0.1",
    "formik": "2.0.1-alpha.2",
    "fstream": "^1.0.12",
    "react": "^16.9.0-alpha.0",
    "react-dom": "^16.9.0-alpha.0",
    "react-redux": "v7.1.0-alpha.4",
    "react-scripts": "3.0.0",
    "redux": "^4.0.1",
    "typeface-roboto": "^0.0.54",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "cucumber": "BABEL_ENV=test npx babel-node --presets=@babel/react,@babel/preset-env node_modules/cucumber/bin/cucumber-js -f node_modules/cucumber-pretty",
    "json-server": "json-server --watch --delay 500 json-server/db.json --id _id --port $npm_package_config_apiPort --routes json-server/routes.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.7.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "chai": "^4.2.0",
    "cucumber": "^5.1.0",
    "cucumber-pretty": "^1.5.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "jsdom": "^15.0.0",
    "node-sass": "^4.12.0",
    "react-hot-loader": "^4.8.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "waait": "^1.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/index.js",
      "!src/serviceWorker.js"
    ]
  },
  "proxy": "http://localhost:4000",
  "config": {
    "clientPort": "3000",
    "apiPort": "4000",
    "baseURL": "/api/tickets/"
  }
}
